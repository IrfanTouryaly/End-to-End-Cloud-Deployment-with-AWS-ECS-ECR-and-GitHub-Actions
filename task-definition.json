{
  "family": "flask-ecs-demo",
  "networkMode": "awsvpc",
  "executionRoleArn": "arn:aws:iam::820242934506:role/ecsTaskExecutionRole",
  "taskRoleArn": "arn:aws:iam::820242934506:role/ecsTaskExecutionRole",
  "cpu": "256",
  "memory": "512",
  "requiresCompatibilities": ["FARGATE"],
  "containerDefinitions": [
    {
      "name": "ecs-container",
      "image": "<IMAGE_URI>",
      "essential": true,
      "portMappings": [
        { "containerPort": 5000, "protocol": "tcp" }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-region": "us-east-1",
          "awslogs-group": "/ecs/flask-ecs-demo",
          "awslogs-stream-prefix": "ecs"
        }
      }
    }
  ]
}

